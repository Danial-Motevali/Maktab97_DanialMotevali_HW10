@model List<AccountModel>

<h1>gg</h1>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Debit</th>
            <th>Credit</th>
            <th>Date</th>
            <th>Note</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in Model)
        {
            <tr>
                <td>@product.name</td>
                <td>@product.debit</td>
                <td>@product.credit</td>
                <td>@product.date</td>
                <td>@product.note</td>
            </tr>
        }
    </tbody>
</table>

<form asp-action="Debit">
    <input type="number" name="number" value="" />
    <button type="submit">Debit</button>
</form>

<form asp-action="Credit">
    <input type="number" name="number" value="" />
    <button type="submit">Credit</button>
</form>

<form asp-action="BackToMenu">
    <input type="hidden" value="" />
    <button type="submit">BackToMenu</button>
</form>

